; calculate the value at the given offset of the fibonacci sequence
(defn fib [n]
  (if (< n 2)
    n
    (+ (fib (- n 2)) (fib (- n 1)))))

(doc fib)

(emit (fib 20) *stdout*)

(defn fib-iter [a b num]
  (if (= num 0)
    b
    (fib-iter (+ a b) a (- num 1))))

; a tail-recursive form of fib
(defn fib-tail [n]
  (fib-iter 1 0 n))

(emit (fib-tail 20) *stdout*)
